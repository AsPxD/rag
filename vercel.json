{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "api/main.py"
    },
    {
      "src": "/rag",
      "dest": "api/main.py"
    },
    {
      "src": "/rag/stream",
      "dest": "api/main.py"
    },
    {
      "src": "/stats",
      "dest": "api/main.py"
    },
    {
      "src": "/docs",
      "dest": "api/main.py"
    },
    {
      "src": "/openapi.json",
      "dest": "api/main.py"
    }
  ],
  "env": {
    "GOOGLE_API_KEY": "@google_api_key"
  }
}
